{
    "name": "mvdstam/php-jwt-middleware",
    "description": "PSR-7 compliant middleware to handle incoming JWT's",
    "license": "MIT",
    "authors": [
        {
            "name": "Max van der Stam",
            "email": "max@vdsdevelopment.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "lcobucci/jwt": "^3.2",
        "psr/http-message": "^1.0",
        "symfony/options-resolver": "^3.0",
        "php": "^7.0",
        "symfony/event-dispatcher": "^3.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Mvdstam\\PhpJwtMiddleware\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdstam\\PhpJwtMiddleware\\Tests\\": "./tests"
        }
    }
}
